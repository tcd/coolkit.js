{
    "private": false,
    "name": "coolkit-js",
    "version": "1.0.0-rc.8",
    "description": "Code I frequently reuse.",
    "author": "Clay Dunston <dunstontc@gmail.com>",
    "license": "MIT",
    "repository": "github:tcd/coolkit.js",
    "bugs": "https://github.com/tcd/coolkit.js/issues",
    "homepage": "https://github.com/tcd/coolkit.js",
    "keywords": [
        "typescript",
        "utilities",
        "util"
    ],
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "files": [
        "./dist"
    ],
    "scripts": {
        "build": "npm run build:clean && npm run build:esm && npm run build:cjs",
        "build:clean": "cross-env rimraf ./dist",
        "build:esm": "cross-env tsc --project ./tsconfig.esm.json",
        "build:cjs": "cross-env tsc --project ./tsconfig.cjs.json",
        "publish:next": "npm publish --tag next",
        "publish:latest": "npm publish --tag latest",
        "test": "cross-env NODE_ENV=test ts-mocha -r tsconfig-paths/register -p ./tsconfig.test.json 'test/**/*.test.ts'"
    },
    "dependencies": {
        "axios": "~1.0.0",
        "colorette": "^2.0.19",
        "lodash.merge": "^4.6.2",
        "mkdirp": "^1.0.4",
        "papaparse": "^5.3.2"
    },
    "devDependencies": {
        "@types/chai": "^4.3.4",
        "@types/lodash.merge": "^4.6.7",
        "@types/mkdirp": "^1.0.2",
        "@types/mocha": "^10.0.1",
        "@types/node": "^17.0.23",
        "@types/papaparse": "^5.3.5",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "chai": "^4.3.7",
        "cross-env": "^7.0.3",
        "eslint": "^8.12.0",
        "eslint-config-dunstontc": "^0.1.0",
        "eslint-plugin-jsdoc": "^39.3.6",
        "rimraf": "^3.0.2",
        "ts-mocha": "^10.0.0",
        "typescript": "^4.8.3"
    }
}
